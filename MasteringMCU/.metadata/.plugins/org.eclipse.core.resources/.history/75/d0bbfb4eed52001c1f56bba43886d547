#include "stm32f4xx_hal.h"
#include "main.h"

void SystemClockConfig(void);
void UART2_Init(void);

UART_HandleTypeDef huart2;	//variável do identificardo do periférico

int main(void)
{
	HAL_Init();
	SystemClockConfig();

	UART2_Init();

	return 0;
}

void SystemClockConfig(void)
{
	/* usar essa função quando forem necessárias configurações especiais no clock*/

}

void UART2_Init(void)
{
	huart2.Instance = UART2; /*!< UART registers base address        */
	huart2.Init.BaudRate = 115200;

}
